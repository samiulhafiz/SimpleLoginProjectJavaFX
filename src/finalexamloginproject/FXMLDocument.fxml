<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="341.0" prefWidth="545.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/20.0.1" fx:controller="finalexamloginproject.FXMLDocumentController">
    <children>
        <Label fx:id="loginMessage" layoutX="164.0" layoutY="39.0" text="Already have an account? Login">
            <font>
                <Font name="System Bold" size="19.0" />
            </font>
        </Label>

        <TextField fx:id="usernameField" layoutX="168.0" layoutY="116.0" prefHeight="26.0" prefWidth="293.0" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="165.0" layoutY="174.0" prefHeight="26.0" prefWidth="294.0" promptText="Password" />
        <Button fx:id="loginButton" layoutX="191.0" layoutY="255.0" onAction="#handleLogin" text="Login" />
        <Button fx:id="registerButton" layoutX="297.0" layoutY="255.0" onAction="#handleRegister" text="Register" />
        <Button fx:id="logoutButton" layoutX="77.0" layoutY="255.0" onAction="#handleLogout" text="Logout" />
        <Button fx:id="deleteUserButton" layoutX="416.0" layoutY="255.0" onAction="#handleDelete" text="Delete" />
        <Text layoutX="84.0" layoutY="133.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
            <font>
                <Font size="16.0" />
            </font>
        </Text>
        <Text layoutX="84.0" layoutY="191.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
            <font>
                <Font size="16.0" />
            </font>
        </Text>
    </children>
</AnchorPane>
